{% extends 'base.html.twig' %}

{% block title %} Regions {% endblock %}

{% block body %}

    <div>
        <h1> Available Regions </h1>
    </div>
    
    {% for region in regions %}
        <h3> {{ region.name }} </h3>
    {% endfor %}

    <div>
        <a href="{{ path('add_region') }}">Add a Region</a>
    </div>

{% endblock %}
